<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="htl day1"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <text
                jcr:primaryType="nt:unstructured"
                jcr:title="Text"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                    margin="{Boolean}false"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <company
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="ompany name description"
                                fieldLabel="company"
                                name="./companynamedescription"
                                useFixedInlineToolbar="{Boolean}true"/>
                            <first-name
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="first name description"
                                fieldLabel="First Name"
                                name="./firstname"
                                required="{Boolean}false"
                                useFixedInlineToolbar="{Boolean}true"/>
                            <last-name
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="last name description"
                                fieldLabel="Last Name"
                                name="./lastname"
                                required="{Boolean}false"
                                useFixedInlineToolbar="{Boolean}true"/>
                            <Email-Address
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="email address description"
                                fieldLabel="Email Address"
                                name="./emailaddress"
                                required="{Boolean}false"
                                useFixedInlineToolbar="{Boolean}true"/>
                            <Title
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="title description"
                                fieldLabel="Title"
                                name="./title"
                                required="{Boolean}true"
                                useFixedInlineToolbar="{Boolean}true"/>
                            <Phone
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="phone no description"
                                fieldLabel="Phone"
                                name="./phone"
                                required="{Boolean}true"
                                useFixedInlineToolbar="{Boolean}true"/>
                            <Cancel-Registration
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="/libs/granite/ui/components/coral/foundation/form/checkbox"
                                checked="{Boolean}true"
                                fieldDescription="cancel set for right side of the check box"
                                name="./no"
                                text="Cancel Registration:"
                                tooltipPosition="top"
                                useFixedInlineToolbar="{Boolean}true"
                                value="enter"/>
                            <Job-Functionr-adio
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="/libs/granite/ui/components/coral/foundation/form/radiogroup"
                                fieldDescription="rselect adio button"
                                fieldLabel="radio button"
                                name="./jobfunction"
                                required="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <campaign_x0020_management
                                        jcr:primaryType="nt:unstructured"
                                        text="campaign management"
                                        value="campaign management"/>
                                    <CRM_x0020_administration
                                        jcr:primaryType="nt:unstructured"
                                        text="crm administration"
                                        value="crm administration"/>
                                    <Email_x0020_Deployment
                                        jcr:primaryType="nt:unstructured"
                                        text="Email Deployment&#xa;"
                                        value="Email Deployment"/>
                                    <Partner
                                        jcr:primaryType="nt:unstructured"
                                        text="partner"
                                        value="partner"/>
                                    <Employee
                                        jcr:primaryType="nt:unstructured"
                                        text="Employee"
                                        value="Employee"/>
                                </items>
                            </Job-Functionr-adio>
                            <Dietary-Requirements
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="text description"
                                fieldLabel="Dietary Requirements"
                                name="./text"
                                required="{Boolean}true"
                                useFixedInlineToolbar="{Boolean}true"/>
                            <Expectation
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="/libs/granite/ui/components/coral/foundation/form/textarea"
                                cols="{Long}8"
                                fieldDescription="expected description"
                                fieldLabel="expectation?"
                                maxlength="{Long}100"
                                name="./expectation"
                                required="{Boolean}true"
                                rows="{Long}5"
                                useFixedInlineToolbar="{Boolean}true"/>
                            <image
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="/libs/granite/ui/components/coral/foundation/form/pathfield"
                                fieldDescription="photo description"
                                fieldLabel="Upload Photo"
                                icon="image"
                                iconSize="small"
                                mimeTypes="image"
                                multiple="{Boolean}true"
                                name="./shoeimage"
                                sizeLimit="{Long}100"/>
                        </items>
                    </column>
                </items>
            </text>
        </items>
    </content>
</jcr:root>
